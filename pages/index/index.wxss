page {
  height: 100%;
}

.test-bg {
  position: fixed;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.bg-image {
  width: 100%;
  height: 100%;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.moto {
  font-size: 32rpx;
  line-height: 70rpx;
  color: #ec5213;
}

.moto-conrainer {
  position: absolute;
  bottom: 100rpx;
  border: 1px solid #ec5213;
  width: 360rpx;
  height: 70rpx;
  border-radius: 5px;
  text-align: center;
}
